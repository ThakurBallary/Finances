    "setupFilesAfterEnv": [
      "./src/config/tests/jest.setup.js",
      "@testing-library/react-native/dont-cleanup-after-each"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native-community|@react-navigation)"
